{
    "manifest_version": 2,
    "name": "Unqork Insert Localhost",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "logo16.png",
        "19": "logo19.png",
        "38": "logo38.png",
        "48": "logo48.png",
        "128": "logo128.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs", "activeTab"]
}
